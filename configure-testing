#define GPSIGNAL_VERSION_TYPE "testing"
#define GPSIGNAL_AUTHORS "Marc Wathelet\nMarc Wathelet (LGIT, Grenoble, France)"
#define GPSIGNAL_DISTRIBUTION "3.3.0-preview"
#define GPSIGNAL_GIT_COMMIT "55f9a141f9f9290767497e96d73bf7a39c55122d"
#endif // GPSIGNAL_VERSION
ENDVERSION
replaceFile $BUILD_DIR/gpsignal/src gpsignalVersion.h
echo "Configure gpsignal"
cd "gpsignal"
echo "$SHELL configure-testing -accept-license -sourcedir $SOURCE_DIR -dest-prefix $BUILD_DIR -builddir $BUILD_DIR -buildsubdir gpsignal -skip-qt-check $COMMANDLINE 2>$TEMP_DIR/error.log 1>$TEMP_DIR/info.log" > $TEMP_DIR/configure
if ! $SHELL $TEMP_DIR/configure; then
  cat $TEMP_DIR/info.log
  cleanexit 2
else
  cat $TEMP_DIR/error.log
fi
cd ..
# Add project to main pro file
echo "          gpsignal \\" >> $BUILD_DIR/geopsypack.pro
