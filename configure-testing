#endif // MSEED_VERSION
ENDVERSION
echo "Configure mseed"
cd "mseed"
echo "$SHELL configure-testing -accept-license -sourcedir $SOURCE_DIR -builddir $BUILD_DIR -buildsubdir mseed -skip-qt-check -dest-prefix $SOURCE_DIR $COMMANDLINE 2>$TEMP_FILE 1>/dev/null" > tmp
if ! $SHELL tmp; then
  cat $TEMP_FILE
