# Useful functions
absolutePath()
{
  PATH=$1
  if [ "${PATH:0:1}" = "/" ]; then
    echo $PATH
  else
    ABS_PATH=$PWD
    while [ "${PATH:0:3}" = "../" ]; do
      ABS_PATH=${ABS_PATH%/[^/]*}
      PATH=${PATH:3}
    done
    echo $ABS_PATH/$PATH
  fi
}

# Arguments: destination directory, file
# New file must be generated in TEMP_DIR
function replaceFile()
{
  if [ -e $1/$2 ] && diff $1/$2 $TEMP_DIR/$2 > /dev/null; then
    rm $TEMP_DIR/$2
  else
    mv $TEMP_DIR/$2 $1
  fi
}

